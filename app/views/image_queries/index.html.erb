<p id="notice"><%= notice %></p>

<h1>Image Queries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Person</th>
      <th>Is match</th>
      <th>Percentage match</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_queries.each do |image_query| %>
      <tr>
        <td><%= image_query.name %></td>
        <td><%= image_query.person.firstname %></td>
        <td><%= image_query.is_match %></td>
        <td><%= image_query.percentage_match %></td>
        <td><%= link_to 'Show', image_query %></td>
        <td><%= link_to 'Edit', edit_image_query_path(image_query) %></td>
        <td><%= link_to 'Destroy', image_query, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image Query', new_image_query_path %>
